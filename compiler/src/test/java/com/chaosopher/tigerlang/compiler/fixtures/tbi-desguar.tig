
        let
            var N_0 := 8
            function printb_1() = 
                (
                    
                    let
                        var lo := 0
                        var hi := (N_0 - 1)
                        var i_2 := lo
                    in
                        (
                            if (i_2 <= hi) 
                            then 
                            while 1 do
                            (
                                (
                                    
                                    let
                                        var lo := 0
                                        var hi := (N_0 - 1)
                                        var j_3 := lo
                                    in
                                        (
                                            
                                            if (j_3 <= hi) 
                                            then
                                            ( 
                                                while 1 do
                                                (
                                                    print(
                                                    if (i_2 > j_3) 
                                                    then "x"
                                                    else "y");
                                                    if (j_3 = hi) 
                                                    then  break
                                                    else j_3 := (j_3 + 1)
                                                )
                                            )
                                        )
                                    end;
                                    print("\n")
                                );
                                if (i_2 = hi) 
                                then  break
                                else i_2 := (i_2 + 1)
                            ) 
                        )
                    end;
                    print("\n")
                )
        in
            (
                printb_1()
            )
        end